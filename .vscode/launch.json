{
    "version": "0.2.0",
    "configurations": [        
        {
            "type": "gdb",
            "request": "attach",
            "gdbpath": "riscv64-unknown-linux-gnu-gdb",
            "name": "Attach to gdbserver",
            "executable": "${env:RAFI_FREEDOM_U_SDK}/work/linux/vmlinux",
            "target": ":8000",
            "remote": true,
            "cwd": "${workspaceRoot}",
            "valuesFormatting": "parseText",
            "printCalls": true
        }
    ]
}